{
  "name": "theme-builder",
  "version": "0.5.1",
  "description": "README",
  "main": "lib/app.js",
  "scripts": {
    "build": "babel --copy-files ./src --out-dir lib",
    "coverage:coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint --color '{src,tests}/**/*.js'",
    "test:update": "jest -u",
    "test": "jest",
    "version": "npm run build",
    "transpile": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Travix-International/ui.git"
  },
  "keywords": [
    "themes",
    "builder",
    "yaml",
    "themes-builder"
  ],
  "author": {
    "name": "Travix International",
    "url": "http://travix.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/Travix-International/theme-builder#readme",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(js)$",
    "transform": {
      ".*": "babel-jest"
    },
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "contributors": [
    {
      "name": "Ivana Viranovska",
      "url": "https://github.com/iwwwi"
    },
    {
      "name": "Artem Riasnianskyi",
      "url": "https://github.com/asci"
    }
  ],
  "dependencies": {
    "js-yaml": "^3.7.0",
    "lodash.merge": "^4.6.0",
    "lodash.template": "^4.4.0",
    "yaml-ast-parser": "0.0.31"
  }
}
